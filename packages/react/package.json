{
  "name": "@tiktok-emojis/react",
  "private": false,
  "version": "1.0.0",
  "license": "MIT",
  "keywords": ["tiktok emojis", "tiktok", "emojis", "react", "svg"],
  "description": "Complete TikTok emojis collection as React components. 46 secret TikTok emojis (smile, happy, angry, etc.) with SVG rendering. Perfect for social media apps, chat interfaces, and engaging user content. Supports React 16.8+",
  "author": "Jun <boqingren@hotmail.com> (https://github.com/boqingren)",
  "homepage": "https://tiktokemojis.click",
  "repository": "https://github.com/boqingren/tiktok-emojis.git",
  "bugs": "https://github.com/boqingren/tiktok-emojis/issues",
  "files": ["dist", "README.md"],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "npx tsup --watch",
    "build": "npx tsup"
  },
  "devDependencies": {
    "@types/react": "16.8.0",
    "@types/react-dom": "16.8.0",
    "tsup": "8.5.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": {
      "import": "./dist/*.mjs",
      "require": "./dist/*.js"
    }
  }
}
