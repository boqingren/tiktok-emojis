  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "npx tsup --watch",
    "build": "npx tsup"
  },
  "devDependencies": {
    "@types/node": "24.0.14",
