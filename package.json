{
  "name": "@tiktok-emojis/root",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "keywords": ["tiktok emojis", "tiktok", "emojis", "react", "vue", "svg"],
  "author": "Jun <boqingren@hotmail.com> (https://github.com/boqingren)",
  "description": "TikTok emojis component library monorepo. Contains React and Vue 3 packages with 46 secret TikTok emojis (smile, happy, angry, etc.) using SVG rendering. Perfect for social media apps and chat interfaces.",
  "homepage": "https://tiktokemojis.click",
  "repository": "https://github.com/boqingren/tiktok-emojis.git",
  "bugs": "https://github.com/boqingren/tiktok-emojis/issues",
  "scripts": {
    "dev": "npx turbo run dev",
    "build": "npx turbo run build",
    "dev:examples": "npx turbo run dev --filter=\"./examples/*\"",
    "build:examples": "npx turbo run build --filter=\"./examples/*\"",
    "dev:packages": "npx turbo run dev --filter=\"./packages/*\"",
    "build:packages": "npx turbo run build --filter=\"./packages/*\"",
    "make:icon": "npx bun ./scripts/src/commands/make-icon.ts",
    "verdaccio": "npx verdaccio"
  },
  "devDependencies": {
    "bun": "1.2.18",
    "prettier": "3.6.2",
    "turbo": "2.5.5",
    "typescript": "5.8.3",
    "verdaccio": "6.1.5"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
